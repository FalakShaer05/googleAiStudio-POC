# API Configuration
# Copy this file to .env and add your API keys

# Google AI API Key (required)
GOOGLE_API_KEY=your-google-api-key-here

# API Keys for authentication (comma-separated)
# Generate secure random keys for production
API_KEYS=dev-api-key-12345,prod-api-key-67890,enterprise-api-key-abcdef

# Background removal is handled by Mosida API (no additional keys needed)

# API Rate Limiting (optional)
API_RATE_LIMIT_REQUESTS=100
API_RATE_LIMIT_WINDOW_MINUTES=60

# File Cleanup Settings (optional)
API_FILE_CLEANUP_HOURS=24
API_MAX_FILE_SIZE_MB=16
